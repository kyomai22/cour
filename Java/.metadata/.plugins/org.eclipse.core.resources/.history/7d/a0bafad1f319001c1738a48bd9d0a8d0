package decouverte;

public class Concatenation {

	public static void main(String[] args) {
		int a=1,b=2;
		
		System.out.println(a+"+"+b+"="+a+b; // 478=478
		//les + servent à concaténer
		
		System.out.println(a+"+"+b+"="+(a+b)); // les parenthèses permettent de faire la somme
		
		System.out.println(a+"+"+b+"="+((a+b)*(a+b)*(a+b)*(a+b)*(a+b)*(a+b)*(a+b)*(a+b)*(a+b)*(a+b)));

	}

}
