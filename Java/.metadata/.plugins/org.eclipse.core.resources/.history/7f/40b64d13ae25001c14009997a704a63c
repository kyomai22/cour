package vecteur;

import java.util.Scanner;

public class DoubleNombre {

	public static void main(String[] args) {
		int []tab=new int[10];
		int nb=0;
		
		System.out.println("Entrez 10 nombres qui seront ensuite doublé");
		
		for (int i=0;i!=10;i++){
			nb=(new Scanner(System.in)).nextInt();
			tab[i]=nb;
		}

	}

}
