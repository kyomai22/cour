package boucles;

public class Punition {

	public static void main(String[] args) {
		int nb1=0,cpt=0,cpt2=0;
		
		
			while(cpt2!=10){
				cpt=0;
				nb1=1;
				nb1=nb1+cpt2;
				System.out.println("");
				System.out.print(nb1+"	");
				while(cpt!=9){					
					nb1=nb1+nb1;
					System.out.print(nb1+"	");
					cpt++;
				}
				cpt2++;
			}
		
		

	}

}
