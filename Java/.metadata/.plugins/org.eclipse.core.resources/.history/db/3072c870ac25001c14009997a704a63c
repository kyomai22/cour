package vecteur;

public class VecteurInit2 {

	public static void main(String[] args) {
		//Ecrire un programme qui remplit un vecteur de 20 éléments  avec les nombres pairs de 0 à 38 et qui 
		//l’affiche sur 4 lignes (5 éléments par ligne)
		
		int []tab=new int[20];
		int nb=0,cpt=0,cpt1=0, cpt2=0,mod=0;
		
		
		while (cpt1!=4){
			cpt=0;
			for(int cpt3=0;cpt3<20;cpt++){
				tab[cpt]=mod;
				System.out.print(" ");
				System.out.print(tab[cpt]);
			while (cpt!=5){
				cpt2=0;
					while (cpt2!=1){
						nb=mod%2-1;
						mod++;
						if(nb==0){
							cpt2++;
						}
					}
				cpt++;
			}
			System.out.println("");
			
			cpt1++;
		}
		
		}
		
		
		
	}

}

