package vecteur;

public class VecteurInit2 {

	public static void main(String[] args) {
		//Ecrire un programme qui remplit un vecteur de 20 éléments  avec les nombres pairs de 0 à 38 et qui 
		//l’affiche sur 4 lignes (5 éléments par ligne)
		
		int []tab=new int[20];
		int nb=0,cpt=0,cpt1=0;
		
		
		while (cpt1!=20){
			cpt=0;
			while (cpt!=5){
				System.out.print("0");
				System.out.print(" ");
				cpt++;
			}
			System.out.println("");
			cpt1++;
		}
		
		
		
		
		
	}

}

