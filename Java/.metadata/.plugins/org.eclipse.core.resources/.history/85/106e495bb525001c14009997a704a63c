package vecteur;

import java.util.Scanner;

public class Inverse {

	public static void main(String[] args) {
		int []tab=new int[10];
		int cpt1=0,cpt2=0,nb=0,tmp=0;
		
		System.out.println("Entrez 10 nombres");
		
		for(cpt1=0;cpt1<10;cpt1++){
			nb=0;
			nb=(new Scanner(System.in)).nextInt();
			tab[cpt1]=nb;
		}
		while(cpt2<10){	
		System.out.print(tab[cpt2]);
		System.out.print("	");
		cpt2++;
	}
		cpt2=0;
		
			for(cpt1=9;cpt1!=0;cpt1--){
				while(cpt2<10){
					tmp=tab[cpt1];
					tab[cpt2]=tmp;
			}
			System.out.println(tab[cpt2]);
		}
			cpt2=0;
			
			}
		

	}
