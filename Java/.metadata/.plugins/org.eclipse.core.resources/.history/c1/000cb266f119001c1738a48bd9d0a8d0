package decouverte;

public class MonDeuxiemeProgramme {

	public static void main(String[] args) {
		int maJolieVariable;
		maJolieVariable=456;
		int a=5,b=85,c=74,d=123;
		int solution;
		
		//addition
		solution = a+ b ;
		System.out.println(a + "+" + b + "=" + solution );
		//division
		solution= d/c;
		System.out.println(d + ":" + c + "=" + solution);
		//modulo
		solution= d%c;
		System.out.println(d + "modulo" + c + solution);
				

	}

}
